  "aircraft-position": {
    "name": "Aircraft Position (ADS-B)",
    "description": "Real-time aircraft position via ADS-B",
    "urlTemplate": "https://api.adsb.one/v2/reg/${term}",
    "resultsField": "ac",
    "resultMapping": {
      "registration": "r",
      "callsign": "flight",
      "type": "t",
      "lat": "lat",
      "lon": "lon",
      "altitude": "alt_baro",
      "speed": "gs",
      "heading": "track",
      "lastSeen": "seen"
    },
    "columns": [
